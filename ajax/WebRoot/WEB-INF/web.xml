<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ajax</display-name>
  <servlet>
    <servlet-name>AjaxServletDemo1</servlet-name>
    <servlet-class>com.vison.servlet.AjaxServletDemo1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CkNameServlet</servlet-name>
    <servlet-class>com.vison.servlet.CkNameServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>AjaxServletDemo1</servlet-name>
    <url-pattern>/ajaxServletDemo1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CkNameServlet</servlet-name>
    <url-pattern>/ckNameServlet</url-pattern>
  </servlet-mapping>
  
</web-app>